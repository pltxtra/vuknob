<machine hint="effect" >
<name>filterbox</name>
<input dimension="midi" channels="1">midi</input>
<input premix="true" dimension="0" channels="2">Stereo</input>
<output dimension="0" channels="2">Stereo</output>

<controller name="cutoff" type="float" min="500.0" max="14000.0" step="1.0" />
<controller name="resonance" type="float" min="0.0" max="127.0" step="1.0" />

<controller name="type" type="integer" min="0" max="1" />

</machine>
